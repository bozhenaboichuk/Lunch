= form_with model: @restaurant do |f|
  = f.label :name
  %br/
  = f.text_area :name, class: "form-control"
  %br/
  = f.label :phone_number
  %br/
  = f.text_area :phone_number, class: "form-control"
  %br/
  = f.label :address
  %br/
  = f.text_area :address, class: "form-control"
  %br/
  = f.submit "save"
